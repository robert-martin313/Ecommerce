@model dynamic 

<div class="row-fluid">
    <div class="span6">
        <h4 class="chart-title">Revenue today</h4>
        <canvas id="revenue-today"></canvas>
    </div>
    <div class="span6">
        <h4 class="chart-title">Revenue in last 7 days</h4>
        <canvas id="revenue-this-week"></canvas>
    </div>
</div>
<div class="row-fluid">
        <div class="span12">
            <div class="pull-right">
            <span class="label peter-river">Mr CMS</span>
            <span class="label emerald" id="amazon">Amazon</span>
            <span class="label alizarin" id="ebay">eBay</span>
                </div>
        </div>  
</div>
@model IList<MrCMS.Web.Apps.Ecommerce.Stock.Entities.Warehouse>
<h4>Warehouse Id Reference</h4>
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Id
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var warehouse in Model)
        {
            <tr>
                <td>
                    @warehouse.Name
                </td>
                <td>
                    @warehouse.Id
                </td>
            </tr>
        }
    </tbody>
</table>
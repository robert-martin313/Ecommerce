@model MrCMS.Entities.Documents.Web.Webpage
<div class="tab-pane" id="postings-tab">
      <div class="row-fluid">
        <div class="span7">
                <h4>Entries (@Model.FormPostings.Count)</h4>
        </div>
        <div class="span5">
            <div class="pull-right btn-group">
                @if (Model.FormPostings.Any())
                {
                    @Html.ActionLink("Delete all entries", "ClearFormData", "Form", new {id = Model.Id}, new {@class = "btn btn-danger", data_toggle = "fb-modal"})
                    @Html.ActionLink("Export Entries", "ExportFormData", "Form", new {id = Model.Id}, new {@class = "btn btn-primary"})
                }
            </div>
        </div>
      </div>
      @Html.Action("Postings", new { id = Model.Id })
</div>
